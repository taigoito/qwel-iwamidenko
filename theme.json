{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "darkest",
					"name": "Darkest",
					"color": "#262626"
				},
				{
					"slug": "darker",
					"name": "Darker",
					"color": "#4d4d4d"
				},
				{
					"slug": "dark",
					"name": "Dark",
					"color": "#808080"
				},
				{
					"slug": "light",
					"name": "Light",
					"color": "#b3b3b3"
				},
				{
					"slug": "lighter",
					"name": "Lighter",
					"color": "#d9d9d9"
				},
				{
					"slug": "lightest",
					"name": "Lightest",
