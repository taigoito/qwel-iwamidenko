				{
					"slug": "secondary",
					"name": "Secondary",
					"color": "#003366"
				}
			]
		},
		"layout": {
			"contentSize": "900px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": null,
			"units": [
				"rem",
				"vh"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"slug": "sans-serif",
					"name": "Sans Serif",
					"fontFamily": "Poppins, \"Noto Sans JP\", sans-serif"
				},
				{
					"slug": "serif",
					"name": "Serif",
					"fontFamily": "\"Yu Mincho\", serif"
				},
				{
					"slug": "system-font",
					"name": "System Font",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "var(--wp--custom--font-size--small)"
				},
				{
					"slug": "medium",
					"size": "var(--wp--custom--font-size--normal)"
				},
				{
					"slug": "large",
					"size": "var(--wp--custom--font-size--medium)"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.5rem, 4vw, 3rem)"
				}
			],
			"dropCap": false
		},
		"custom": {
			"spacing": {
				"container-pad": "clamp(1.5rem, 4%, 36px)",
				"container-pad-wide": "clamp(1.5rem, 4%, 48px)",
				"x-small": "0.375rem",
				"small": "0.75rem",
				"medium": "1.5rem",
				"large": "3rem",
				"x-large": "6rem"
			},
			"font-size": {
				"small": "0.875rem",
				"normal": "1rem",
				"medium": "1.17rem",
				"large": "1.33rem",
				"x-large": "1.5rem",
				"xx-large": "2rem"
			},
			"line-height": {
				"x-small": "1.2",
				"small": "1.4",
				"medium": "1.6",
				"large": "1.8",
				"x-large": "2"

			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "var(--wp--custom--line-height--large)"
		},
		"blocks": {
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--custom--font-size--small)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--custom--font-size--small)"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--font-size--x-large)",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--normal)",
					"textTransform": "capitalize",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--normal)",
					"textTransform": "capitalize",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--medium-dark)"
				}
			}
		}
	},
